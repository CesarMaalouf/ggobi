<plugin name="JVM" load="immediate">
   <description>Loads the Java Virtual Machine for use by other plugins</description>
   <author>Duncan Temple Lang</author>
   <dll name="plugins/JVM/JVM.so">
    <init onLoad="loadJVM" />
  </dll>
</plugin>

