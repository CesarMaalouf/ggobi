LD_LIBRARY_PATH=$GGOBI_HOME/lib:@XML_LIB_DIRS_PATH@:${LD_LIBRARY_PATH}
export LD_LIBRARY_PATH

if test `@UNAME@` = "MINGW32_NT-5.1" ; then
 PATH=${GGOBI_HOME}/lib:@XML_LIB_DIRS_PATH@:${PATH}
 export PATH
fi

if test `@UNAME@` = "IRIX64" ; then
 LD_LIBRARYN32_PATH=${GGOBI_HOME}/lib:@XML_LIB_DIRS_PATH@:${LD_LIBRARYN32_PATH}
 export LD_LIBRARYN32_PATH
fi

if test `@UNAME@` = "Darwin" ; then
 DYLD_LIBRARY_PATH=${GGOBI_HOME}/lib:@XML_LIB_DIRS_PATH@:${DYLD_LIBRARY_PATH}
 export DYLD_LIBRARY_PATH
fi
