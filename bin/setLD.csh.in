if(${?LD_LIBRARY_PATH}) then
 setenv LD_LIBRARY_PATH @GGOBI_HOME@/lib:${LD_LIBRARY_PATH}:
else
 setenv LD_LIBRARY_PATH @GGOBI_HOME@/lib
endif

set OS=`@UNAME@`
if ("${OS}" == "IRIX64") then
 if(${?LD_LIBRARYN32_PATH}) 
 then
   setenv LD_LIBRARYN32_PATH @GGOBI_HOME@/lib:${LD_LIBRARYN32_PATH}
 else
   setenv LD_LIBRARYN32_PATH @GGOBI_HOME@/lib
 endif
endif

if ("${OS}" == "Darwin") then
 if(${?DYLD_LIBRARY_PATH}) 
 then
   setenv DYLD_LIBRARY_PATH @GGOBI_HOME@/lib:${DYLD_LIBRARY_PATH}
 else
   setenv DYLD_LIBRARY_PATH @GGOBI_HOME@/lib
 endif
endif
